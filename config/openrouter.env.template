# OpenRouter Configuration Template
# Copy this file to .env in the root directory and fill in your actual API key

# Your OpenRouter API Key (get from https://openrouter.ai/keys)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# AI Model Configuration for DeepSeek R1 Free
DEFAULT_AI_MODEL=openrouter/deepseek/deepseek-r1-free
BACKUP_AI_MODEL=openrouter/deepseek/deepseek-chat

# Cursor IDE Configuration
CURSOR_AI_MODEL=openrouter/deepseek/deepseek-r1-free
CURSOR_API_BASE_URL=https://openrouter.ai/api/v1

# Other optional API Keys
ANTHROPIC_API_KEY=your_anthropic_key_here
OPENAI_API_KEY=your_openai_key_here 